                    android:textColor="@drawable/bg_text_frequencies"
                    android:textSize="@dimen/text_size_large_17"
                    app:fontText="fonts/MyriadPro_Regular.otf" />

                <com.Meditation.Sounds.frequencies.views.CustomFontButton
                    android:id="@+id/btnDot"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/bg_btn_frequencies"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:letterSpacing="0"
                    android:minHeight="0dp"
                    android:paddingVertical="14dp"
                    android:stateListAnimator="@null"
                    android:text="."
                    android:textAllCaps="false"
                    android:textColor="@drawable/bg_text_frequencies"
                    android:textSize="@dimen/text_size_large_17"
                    app:fontText="fonts/MyriadPro_Regular.otf" />
            </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <com.Meditation.Sounds.frequencies.views.CustomFontButton
                    android:id="@+id/btn3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/bg_btn_frequencies"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:letterSpacing="0"
                    android:minHeight="0dp"
                    android:paddingVertical="14dp"
                    android:stateListAnimator="@null"
                    android:text="3"
                    android:textAllCaps="false"
                    android:textColor="@drawable/bg_text_frequencies"
                    android:textSize="@dimen/text_size_large_17"
                    app:fontText="fonts/MyriadPro_Regular.otf" />

                <com.Meditation.Sounds.frequencies.views.CustomFontButton
                    android:id="@+id/btn000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/bg_btn_frequencies"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:letterSpacing="0"
                    android:minHeight="0dp"
